{
  "env": {
    "browser": false,
    "commonjs": false,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "no-new": "off",
    "semi": [
      2,
      "never"
    ]
  }
}
